#include<iostream>
using namespace std;
int main()
{
    int num,first=0,second=1,third;
    cout<<"Enter the number of terms to be printed in the fibonacci series"<<endl;
    cin>>num;
    cout<<"Fibonacci series:"<<endl;
    for(int i=0;i<num;i++)
    {
        cout<<first<<endl;
        third=first+second;
        first=second;
        second=third;
    }
    return 0;
}
